<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<SplitPane fx:id="splitPane" dividerPositions="0.5" prefHeight="331.0" prefWidth="500.0" stylesheets="@../style/darkStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TwitterGetInfo">
   <items>
      <VBox maxWidth="150.0" minWidth="150.0" prefWidth="150.0">
         <children>
            <ComboBox fx:id="boxCountry" prefWidth="150.0" promptText="chose" />
          <Button fx:id="buttonProcess" mnemonicParsing="false" onAction="#startCalculate" prefWidth="150.0" text="SEARCH" />
            <Button fx:id="listOf" mnemonicParsing="false" onAction="#listOfTweets" prefWidth="150.0" text="LIST" />
            <Button fx:id="aboutDev" mnemonicParsing="false" onAction="#about" prefWidth="150.0" text="ABOUT" />
            <Button fx:id="histoBtn" maxWidth="150.0" minWidth="150.0" mnemonicParsing="false" onAction="#showHistogram" prefWidth="150.0" text="HISTOGRAM" />
            <Button fx:id="exportToExcl" maxWidth="150.0" minWidth="150.0" mnemonicParsing="false" onAction="#saveToExcel" prefWidth="150.0" text="SAVE TO EXCEL" />
         </children>
      </VBox>
      <AnchorPane fx:id="absolutePane" style="-fx-background-color: black;" stylesheets="@../style/darkStyle.css" />
   </items>
</SplitPane>
